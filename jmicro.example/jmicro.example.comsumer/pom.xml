	<dependencies>
		<dependency>
			<groupId>org.jmicro</groupId>
			<artifactId>jmicro.build</artifactId>
		</dependency>
		<dependency>
			<groupId>org.jmicro</groupId>
			<artifactId>jmicro.example.api</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>
		<!-- <dependency>
			<groupId>org.jmicro</groupId>
			<artifactId>jmicro.mybatis</artifactId>
		</dependency> -->
		
		<dependency>
			<groupId>org.jmicro</groupId>
			<artifactId>jmicro.idgenerator</artifactId>
		</dependency>
		 <dependency>
			 <groupId>org.jmicro</groupId>
			 <artifactId>jmicro.transport.netty.client</artifactId>
	    </dependency>
		<dependency>
			<groupId>org.jmicro</groupId>
			<artifactId>jmicro.gateway.client</artifactId>
		</dependency>
		<dependency>
