				 <groupId>org.jmicro</groupId>
				 <artifactId>jmicro.mongodb</artifactId>
	        </dependency> -->
	        <dependency>
			<groupId>org.jmicro</groupId>
			<artifactId>jmicro.test</artifactId>
			<scope>test</scope>
		</dependency>
		
		 <dependency>
			<groupId>org.jmicro</groupId>
			<artifactId>jmicro.choreography.api</artifactId>
		</dependency>
		
		<dependency>
